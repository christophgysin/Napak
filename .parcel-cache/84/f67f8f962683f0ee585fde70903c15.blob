/* Ticks */
.page-ticks {
	overflow-y: auto;
	min-height: 100%;
}

.tick-list table {
	width: 100%;
	margin: 10px 0;
	border-spacing: 0;
	border-collapse: separate;
}

.tick-list table th {
	text-align: left;
	color: inherit;
	font-family: 'IBM Plex Mono';
	font-size: 12px;
	text-transform: uppercase;
	font-weight: 100;
	border-bottom: 1px solid var(--color-white-dim);
	text-indent: 5px;
}

.tick-list table tr.header {
	background: transparent;
	margin-bottom: 10px;
}
.tick-list table tr.header td {
	border-bottom: 1px solid var(--color-white-dim)
}

.page-tick TR.header TH:not(:first-of-type) {
	padding-top: 10px;
}

.page-tick TR.header:not(:first-of-type) TH {
	padding-top: 10px;
}

.page-tick TD {
	white-space: pre;
	position: relative;
	overflow: hidden;
	text-overflow: ellipsis;
	padding: 3px;
	font-size: 12px;text-transform: capitalize;
}

.grade-legend {
	background:#0f8;
	color: #000;
	width: 25px;
	display: flex;
	border-radius: 20px;
	justify-content: center;
	align-items: center;
}